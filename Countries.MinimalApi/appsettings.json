{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"EndpointDefaults": {
			"Protocols": "Http1AndHttp2AndHttp3"
		}
	},
	"Serilog": {
		"Using": [
			"Serilog.Sinks.ApplicationInsights"
		],
		"MinimumLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		},
		"WriteTo": [
			{
				"Name": "ApplicationInsights",
				"Args": {
					"connectionString": "InstrumentationKey=e91424fa-08af-44bf-8949-1cc08b9a3a40;IngestionEndpoint=https://westus-0.in.applicationinsights.azure.com/;LiveEndpoint=https://westus.livediagnostics.monitor.azure.com/",
					"telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
				}
			}
		],
		"Enrich": [
			"FromLogContext"
		],
		"Properties": {
			"Application": "DemoAPI"
		}
	},
	"ApplicationInsights": {
		"ConnectionString": "{YourConnectionString}"
	},
	"ConnectionStrings": {
		"Db": "Data Source=(LocalDB)\\MSSQLLocalDB;Initial Catalog=Db;MultipleActiveResultSets=true;Encrypt=false;timeout=30;",
		"Db1": "Data Source=(LocalDB)\\MSSQLLocalDB;Initial Catalog=Db1;MultipleActiveResultSets=true;Encrypt=false;timeout=30;",
		"Db2": "Data Source=(LocalDB)\\MSSQLLocalDB;Initial Catalog=Db2;MultipleActiveResultSets=true;Encrypt=false;timeout=30;"
	}
}  